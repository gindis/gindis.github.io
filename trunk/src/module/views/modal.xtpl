<div class="ui-dialog">
  <div class="ui-dialog-cnt">
    <div class="ui-dialog-bd">
      <h4 class="tit">{{title}}</h4>
      <div class="con">{{{content}}}</div>
    </div>
    <div class="ui-dialog-ft ui-btn-group">
    {{#each (button)}}
      <button type="button"  {{#if (select===xindex)}}class="select"{{/if}}  data-role="button" id="dialogButton{{xindex}}">{{title}}</button>
    {{/each}}
    </div>
  </div>
</div>