<header style="background-image: url({{picture.url}});" class="header">
  <h2 class="title">{{title}}</h2>
  <p class="partition">—</p>
  <p class="say">{{subTitle}}</p>
  <p class="clothes"><span class="icon">{{matchNum}}</span></p>
</header>
<section class="container">
  {{#if (type === 1 && dynamics && (dynamics.list.length > 0) )}}
  {{#each (dynamics.list)}}
  <div class="post {{#if (xindex !== (dynamics.list.length - 1))}}m8{{/if}}">
    <div class="head">
      <div class="avatar"><a href="./people.html?uid={{match.user.id}}"><img src="{{match.user.head.url}}" alt=""></a></div>
      <div class="information">
        <div class="name">{{match.user.name}}</div>
        <div class="attr">{{match.user.area.name}}/{{match.user.height}}CM/{{match.user.hair.name}}</div>
        <a class="follow" href="javascript:;"></a>
      </div>
    </div>
    {{#if (match.user.cover.url !== '')}}
    <div class="cover">
      <img src="{{match.user.cover.url}}" alt="">
    </div>
    {{/if}}
    <div class="description">{{match.description}}</div>
    {{#if (match.prodList.length > 0)}}
    <div class="goods">
      <ul class="list">
        {{#each (match.prodList)}}
        <li>
          <div style="background-image: url({{picture.url}});" class="cover"></div>
          <div class="name nowrap">{{name}}</div>
          <div class="price">&yen; {{price}}</div>
          <a class="link" href="./detail-goods.html?id={{id}}"></a>
        </li>
        {{/each}}
      </ul>
    </div>
    {{/if}}
  </div><!--/.post-->
  {{/each}}
  {{elseif (type === 2 && dynamics && (dynamics.list.length > 0))}}
  <div class="post">
    <div class="matchs">
      <div class="matchs-inner">
        {{#each (dynamics.list)}}
        <div class="item">
          <div class="item-head">
            <div class="avatar">{{#if (macth.user.cover.url)}}<img src="{{macth.user.cover.url}}" alt="">{{/if}}</div>
            <div class="information">
              <div class="name">{{match.user.name}}</div>
              <div class="attr">{{match.user.area.name}}/{{match.user.height}}CM</div>
            </div>
          </div>
          <div style="background-image: url({{match.picture.url}});" class="cover"><span class="fav">{{match.praiseCount}}</span><a class="link" href="./detail-goods.html?id={{macth.id}}"></a></div>
        </div>
        {{/each}}
      </div>
    </div>
  </div>
  {{elseif (type === 3)}}
  <div class="post">
    <div class="description ui-border-b">{{content}}</div>
    {{#if (products && (products.list.length > 0))}}
    {{#each (products.list)}}
    <div class="sku ui-border-b">
      <div class="sku-head">{{name}}</div>
      <div class="body">
        {{#if (picture.url)}}
        <img width="100%" src="{{picture.url}}" alt="{{name}}">
        {{/if}}
        <p>{{description}}</p>
      </div>
      <div class="like-item">
        <div class="cover">{{#if (picture.url)}}<img width="100%" src="{{picture.url}}" alt="{{name}}">{{/if}}</div>
        <div class="information">
          <div class="name nowrap">{{name}}</div>
          <div class="price">&yen; {{price}}</div>
          <button class="ui-btn btn-buy">购买</button>
        </div>
        <a class="link" href="./detail-goods.html?id={{id}}"></a>
      </div>
    </div><!--/.sku-->
    {{/each}}
    {{/if}}
  </div>
  {{/if}}
  <div class="separator-wrap">
  <div class="separator">
    <div class="line ui-border-b"></div>
    <div class="n"><span class="pageview">{{viewNum}}</span><span class="follows">{{followNum}}</span></div>
  </div><!--/.separator-->
  </div>
  <div class="author">
    <div class="title">专题作者</div>
    <div class="avatar"><a href="./people.html?uid={{user.id}}">{{#if (user.cover.url)}}<img src="{{user.cover.url}}" alt="">{{/if}}</a></div>
  </div>
  {{#if (subjects.length > 0)}}
  <div class="separator-wrap">
  <div class="separator">
    <div class="line ui-border-b"></div>
    <div class="n">推荐专题</div>
  </div><!--/.separator-->
  </div>
  <div class="topics-wrap">
  <div class="topics">
    {{#each (subjects)}}
    <div style="background-image: url({{picture.url}});" class="topic-item">
      <div class="title {{#if (type === 1)}}t1{{elseif (type=== 2)}}t1{{else}}t2{{/if}}">{{#if (type === 1)}}穿搭示范{{elseif (type=== 2)}}搭配专题{{else}}单品合集{{/if}}</div>
      <div class="body">
        <p>{{title}}</p>
        <p class="bwt">{{subTitle}}</p>
      </div>
    </div>
    {{/each}}
  </div>
  <div class="more">
    没有更多
  </div>
  {{/if}}
  </div>
</section>