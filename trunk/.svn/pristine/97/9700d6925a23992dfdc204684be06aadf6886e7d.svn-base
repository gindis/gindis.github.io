.ui-border-b {
  border-bottom: 1px solid #EAF0F9;
}

@media screen and (-webkit-min-device-pixel-ratio: 2) {
  .ui-border-b {
    border: none;
  }

  .ui-border-b {
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0.5, transparent), color-stop(0.5, #EAF0F9), to(#EAF0F9));
  }
}
