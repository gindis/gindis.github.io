.reddot, .reddot-border, .reddot-s {
  position: relative;
  display: inline-block;
  line-height: 22px;
  padding: 0 6px;
}
.reddot:after, .reddot-border:after, .reddot-s:after {
  content: '';
  position: absolute;
  display: block;
  width: 8px;
  height: 8px;
  background-color: #f76249;
  border-radius: 5px;
  right: -3px;
  top: -3px;
  background-clip: padding-box;
}

/* 带白边的 */
.reddot-border:before {
  content: '';
  position: absolute;
  display: block;
  width: 8px;
  height: 8px;
  background-color: #fff;
  border-radius: 5px;
  right: -4px;
  top: -4px;
  background-clip: padding-box;
  padding: 1px;
}

/* 小号的 */
