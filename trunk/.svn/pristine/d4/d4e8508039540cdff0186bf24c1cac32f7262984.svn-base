@import

    "mixins",
    "variables";

@font-face {font-family: "iconfont";
    src: url(../common/font/iconfont.ttf) format('truetype'); 
}
.ui-icon,[class^="ui-icon-"] {
    @include icon;
}

.ui-icon-close:before { content: $icon-close;}
.ui-icon-search:before { content: $icon-search;}
.ui-icon-return:before { content: $icon-return;}
.ui-icon-close,
.ui-icon-search{
    color: $icon-color;
}

